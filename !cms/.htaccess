RewriteEngine on
RewriteRule ^([a-zA-Z0-9_-]+)$ index.php?system=$1 [NC]
RewriteRule ^([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+)$ index.php?system=$1&section=$2 [NC]
RewriteRule ^([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+)$ index.php?system=$1&section=$2&var1=$3 [NC]
RewriteRule ^([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+)$ index.php?system=$1&section=$2&var1=$3&var2=$4 [NC]
RewriteRule ^([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+)$ index.php?system=$1&section=$2&var1=$3&var2=$4&var3=$5 [NC]
RewriteRule ^([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+)$ index.php?system=$1&section=$2&var1=$3&var2=$4&var3=$5&var4=$6 [NC]
RewriteRule ^([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+),([a-zA-Z0-9_-]+)$ index.php?system=$1&section=$2&var1=$3&var2=$4&var3=$5&var4=$6&var5=$7 [NC]